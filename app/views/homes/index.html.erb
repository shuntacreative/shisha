<header class='top-page-header'>
  
  <div class='nav'>
    <ul class='lists-right'>
      
      <% if user_signed_in? %>
       <li><%= link_to current_user.nickname, user_path(current_user), class: "user-nickname"  %></li>
       <li><%= link_to 'logout', destroy_user_session_path, method: :delete %></li>
      <% else %>
       <li><%= link_to 'ログイン', new_user_session_path, class: "login" %></li>
       <li><%= link_to '新規登録', new_user_registration_path, class: "sign-up" %></li>
      <% end %>
      <div>
      <li><%= link_to '写真投稿', new_photo_path %></li>
      </div>
      <div>
      <li><%= link_to 'みんなの投稿', photos_path %></li>
      </div>
      <div>
      <li><%= link_to 'ホームへ', root_path %></li>
      </div>

    </ul>
  </div>
</header>
<h2>home</h2>


<% User.all.each do |user| %>
<div>
  <%= link_to user.nickname, user_path(user) %>
</div>
<% end %>

<%= render "shared/footer" %>
