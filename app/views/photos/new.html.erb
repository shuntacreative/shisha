<%= render "shared/header" %>
<h2>写真投稿</h2>

<%= form_with model: @photo, local: true do |f| %>

<div>
<%= f.file_field :image %>
</div>

<div>
<%= f.text_area :title, placeholder:"shop name" %>
</div>

<div>
<%= f.text_area :place, placeholder:"shop place" %>
</div>

<div>
<%= f.text_area :description, placeholder:"description or memo" %>
</div>

<%= f.submit %>
 

<% end %>

<%= render "shared/footer" %>

