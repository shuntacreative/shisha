

<header class='top-page-header'>
  <div class='search-bar-contents'>
    <%= link_to image_tag("logoshisha.png", class:"shisha-icon"), root_path %>
    <form class="search-form" action="#">
      <input class='input-box' placeholder='search'>
      <button class="search-button">
        <%= image_tag "search.png", class:"search-icon" %>
      </button>
    </form>
  </div>
  <div class='nav'>
    <ul class='lists-right'>
      
      <% if user_signed_in? %>
       <li><%= link_to current_user.nickname, user_path(current_user), class: "headlist"  %></li>
       <li><%= link_to 'logout', destroy_user_session_path, method: :delete, class: "headlist" %></li>
      <% else %>
       <li><%= link_to 'login', new_user_session_path, class: "headlist" %></li>
       <li><%= link_to 'sign up', new_user_registration_path, class: "headlist" %></li>
      <% end %>
      <div>
      <li><%= link_to 'new post', new_photo_path, class: "headlist" %></li>
      </div>
      <div>
      <li><%= link_to 'find', photos_path, class: "headlist" %></li>
      </div>

    </ul>
  </div>
</header>